{
  "name": "pull-request-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jimdo/ui": "^21.4.0",
    "apollo-boost": "^0.1.16",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
